{
  "name": "crawler",
  "version": "1.1.8",
  "description": "Crawler is capable of scraping/crawling any website with a heavily customized puppeteer browser or via plain http requests using Got",
  "repository": {
    "type": "git",
    "url": "https://github.com/NikolaiT/Crawling-Infrastructure"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "docker-push": "docker login --username=tschachn && docker push tschachn/crawl_worker",
    "docker-build": "tsc && docker-compose build",
    "up": "docker-compose up -d",
    "size": "find . -type d -name node_modules |  grep -v node_modules.*node_modules | tr '\\n' '\\0' |  xargs -0 du -sch",
    "test": "mocha --timeout 3000000 -r ts-node/register test/integration_tests.ts"
  },
  "keywords": [
    "scraping",
    "crawling",
    "distributed",
    "scalable",
    "stealthy",
    "undetected",
    "web-scraping"
  ],
  "homepage": "https://scrapeulous.com/",
  "author": "Nikolai Tschacher <contact@scrapeulous.com> (https://scrapeulous.com)",
  "license": "GNU AFFERO GENERAL PUBLIC LICENSE",
  "_moduleAliases": {
    "@lib": "dist/lib"
  },
  "dependencies": {
    "aws-sdk": "^2.600.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "chrome-aws-lambda": "^5.2.0",
    "clipboardy": "^2.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "detect-newline": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fingerprintjs2": "^2.1.2",
    "glob": "^7.1.6",
    "got": "^9.6.0",
    "md5": "^2.3.0",
    "minimist": "^1.2.5",
    "module-alias": "^2.2.2",
    "mongoose": "^5.8.7",
    "proxy-chain": "^0.4.5",
    "puppeteer-core": "^5.2.1",
    "puppeteer-extra": "^3.1.15",
    "puppeteer-extra-plugin-recaptcha": "^3.1.17",
    "puppeteer-extra-plugin-stealth": "^2.6.1",
    "tough-cookie": "^3.0.1",
    "tslib": "^1.10.0",
    "tunnel": "0.0.6",
    "user-agents": "^1.0.509",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/aws-lambda": "^8.10.36",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.7",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/got": "^9.6.9",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^12.12.10",
    "@types/puppeteer": "^2.0.0",
    "@types/puppeteer-core": "^2.0.0",
    "@types/tough-cookie": "^2.3.6",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "puppeteer": "^2.1.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  }
}
